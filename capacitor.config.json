{
  "appId": "com.slappynerds.game",
  "appName": "Slappy Nerds",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "server": {
    "url": "https://06c760a0-107b-47e2-bbe3-dbf285ac4946.lovableproject.com?forceHideBadge=true",
    "cleartext": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#000000",
