    "Keyboard": {
      "resize": "body",
      "style": "DARK",
      "resizeOnFullScreen": true
    }
  },
  "ios": {
    "scheme": "Slappy Nerds",
    "backgroundColor": "#000000"
  },
  "android": {
    "backgroundColor": "#000000",
    "allowMixedContent": true,
    "captureInput": true
  }
}